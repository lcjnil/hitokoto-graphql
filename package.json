{
  "name": "hitokoto",
  "version": "0.0.1",
  "description": "Hitokoto, a graphql version",
  "main": "index.js",
  "scripts": {
    "start": "postgraphql --schema hitokoto",
    "init": "psql -f ./schema/schema.sql && psql -f ./schema/role.sql",
    "drop": "psql -f ./schema/drop.sql",
    "init:data": "psql -f ./schema/data.sql"
